    <rules>
        <fullName>Interested Tradeshow Lead Notification - Atlantic</fullName>
        <actions>
            <name>Interested_Tradeshow_Lead_NotificationAtlantic</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 and 4 AND (2 or 3)</booleanFilter>
        <criteriaItems>
            <field>CampaignMember.Status</field>
            <operation>equals</operation>
            <value>Interested</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Lead_Owner_Role__c</field>
            <operation>startsWith</operation>
            <value>atlantic r</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Contact_Owner_Role__c</field>
            <operation>startsWith</operation>
            <value>atlantic r</value>
        </criteriaItems>
        <criteriaItems>
            <field>CampaignMember.LastModifiedById</field>
            <operation>equals</operation>
            <value>Tina Fairing,Caroline McGinnis,Ann Marie Scheuer,Geof Capodanno,maria mastrocola</value>
        </criteriaItems>
        <description>Sends an alert to Account Executive when a campaign member is added to a campaign and has indicated interest in our services</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
